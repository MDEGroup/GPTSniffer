


public class PowerTable{
    public static void main(String[] args) {
        System.out.println("a    b    pow(a,b)");
        int a, b;
        a = 1;
        b = 2;
        System.out.println(a + "    " + b + "    " + (int) Math.pow(a, b));
        a++;
        b++;
        System.out.println(a + "    " + b + "    " + (int) Math.pow(a, b));
        a++;
        b++;
        System.out.println(a + "    " + b + "    " + (int) Math.pow(a, b));
        a++;
        b++;
        System.out.println(a + "    " + b + "    " + (int) Math.pow(a, b));
        a++;
        b++;
        System.out.println(a + "    " + b + "    " + (int) Math.pow(a, b));
    }
}
