import java.util.Scanner;public class LinearEquationSolver {  public static void main(String[] args) {    Scanner input = new Scanner(System.in);    // Prompt the user to enter values for a, b, c, d, e, and f    System.out.print("Enter a, b, c, d, e, f: ");    double a = input.nextDouble();    double b = input.nextDouble();    double c = input.nextDouble();    double d = input.nextDouble();    double e = input.nextDouble();    double f = input.nextDouble();    // Compute the solution of the linear equation    double denominator = a * d - b * c;    double x = (e * d - b * f) / denominator;    double y = (a * f - e * c) / denominator;    // Display the result    if (denominator == 0) {      System.out.println("The equation has no solution.");    } else {      System.out.printf("x is %.2f and y is %.2f%n", x, y);    }  }}